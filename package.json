{
  "name": "Netsave",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package && cd ios && pod install && cd ..",
    "lint": "eslint .",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "ios:dev": "react-native run-ios --scheme Netsave-Dev --configuration DebugDev --simulator='iPhone SE (2nd generation)'",
    "ios:staging": "react-native run-ios --scheme Netsave-Staging --configuration DebugStaging --simulator='iPhone SE (2nd generation)'",
    "ios:production": "react-native run-ios --scheme NetSave --configuration DebugProduction --simulator='iPhone SE (2nd generation)'",
    "android:dev": "./scripts/android-run-dev.sh",
    "android:staging": "./scripts/android-run-staging.sh",
    "android:production": "./scripts/android-run-production.sh",
    "android:build:dev": "./scripts/android-build-dev.sh",
    "android:build:staging": "./scripts/android-build-staging.sh",
    "android:build:production": "./scripts/android-build-production.sh",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "type-check": "tsc"
  },
  "pre-commit": [],
